<UserControl x:Class="LGP.Components.Database.Oracle.Preferences"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" mc:Ignorable="d" d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="200*" />
            <RowDefinition Height="78*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="110" />
            <ColumnDefinition Width="166*" />
            <ColumnDefinition Width="253*" />
        </Grid.ColumnDefinitions>
        <Label Content="{Resx ResxName=LGP.Components.Database.Oracle.Properties.Resources, Key=Port}" Grid.Row="0" Height="28" HorizontalAlignment="Right" Margin="10" Name="PortNumberLabel" VerticalAlignment="Top"  />
        <TextBox Grid.Column="1" Grid.Row="0" Height="23" HorizontalAlignment="Stretch" Margin="10" Name="textBox1" VerticalAlignment="Top" />
        <Button Content="{Resx ResxName=LGP.Components.Database.Oracle.Properties.Resources, Key=Default}" Grid.Column="2" Grid.Row="0" Height="23" HorizontalAlignment="Left" Margin="10" Name="SetDefaultPortButtonClick" VerticalAlignment="Top" Width="75" Click="SetDefaultPortButtonClickClick" />
    </Grid>
</UserControl>